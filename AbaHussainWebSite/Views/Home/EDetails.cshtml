<script>


      $(document).ready(function ()
    {
       $("#remove a").click(function (e) {
           e.preventDefault();
           var id = $(this).attr("id");

           var MSG = confirm("Are you sure you want to delete this Record?");

           if (MSG) {

               $.ajax({
                   type:"POST",
                   url: '/Deleting/Deletebranchdetails/id',
                   data: { id: id },
                   success: function () {
                       $("#row_" + id).remove();
                   },
                   error: function () {

                   }
               });
           }
       })
   });

</script>
